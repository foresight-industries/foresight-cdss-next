{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        ".vercel/**",
        "!.vercel/cache/**",
        "tsconfig.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ]
    },
    "build:functions": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "*.js",
        "*.d.ts",
        "tsconfig.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "jest.config.*",
        "vitest.config.*",
        ".env*"
      ]
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": ["eslint-report.json"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".eslintrc*",
        "eslint.config.*"
      ]
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": ["eslint-report.json"]
    },
    "check-types": {
      "dependsOn": ["^build"],
      "outputs": ["tsconfig.tsbuildinfo"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig*.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "dependsOn": ["^build"],
      "outputs": ["src/schema.ts", "drizzle/**"],
      "inputs": [
        "drizzle.config.*",
        ".env*"
      ]
    },
    "db:push": {
      "dependsOn": ["db:generate"],
      "cache": false,
      "inputs": [
        "drizzle.config.*",
        "src/schema.ts",
        ".env*"
      ]
    },
    "db:migrate": {
      "dependsOn": ["^build", "db:generate"],
      "cache": false,
      "inputs": [
        "drizzle.config.*",
        "migrations/**",
        ".env*"
      ]
    },
    "db:studio": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".prettierrc*",
        "prettier.config.*"
      ]
    },
    "format:check": {
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".prettierrc*",
        "prettier.config.*"
      ]
    },
    "test:e2e": {
      "dependsOn": ["@foresight-cdss-next/web#build"],
      "cache": false,
      "inputs": [
        "$TURBO_DEFAULT$",
        "cypress.config.*",
        ".env*"
      ]
    }
  },
  "globalDependencies": [
    ".env*",
    "turbo.json",
    "package.json",
    "tsconfig.json"
  ],
  "globalEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "DATABASE_URL",
    "AWS_REGION",
    "AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "GEMINI_API_KEY",
    "DOSESPOT_USER_ID",
    "DOSESPOT_SUBSCRIPTION_KEY",
    "DOSESPOT_BASE_URL",
    "DOSESPOT_CLINIC_ID",
    "DOSESPOT_CLINIC_KEY",
    "DOSESPOT_ADMIN_USER_ID",
    "DOSESPOT_WEBHOOK_SECRET",
    "DOSESPOT_PROVIDER_ID",
    "POSTGRES_URL",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_DATABASE",
    "SUPABASE_JWT_SECRET",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "CLERK_SECRET_KEY",
    "SUPABASE_PROJECT_REF_PROD",
    "STRIPE_WEBHOOK_SECRET",
    "STRIPE_PUBLISHABLE_KEY",
    "STRIPE_SECRET_KEY",
    "CLERK_WEBHOOK_SECRET",
    "SVIX_ENV_ID",
    "SVIX_API_KEY",
    "RESEND_API_KEY",
    "SUPABASE_SERVICE_ROLE_KEY",
    "SUPABASE_URL",
    "SUPABASE_ANON_KEY",
    "CLAIM_MD_API_KEY",
    "OPENAI_API_KEY",
    "AWS_ACCOUNT_ID",
    "DATABASE_NAME",
    "DATABASE_CLUSTER_ARN",
    "DATABASE_SECRET_ARN",
    "DOCUMENTS_BUCKET_NAME",
    "REDIS_LANGCACHE_API_KEY",
    "REDIS_DB_USERNAME",
    "REDIS_CLUSTER_MODE",
    "REDIS_DB_URL",
    "REDIS_DB_PASSWORD",
    "STEDI_API_KEY",
    "WEBHOOK_DELIVERY_QUEUE_URL"
  ]
}
