# Add these to your .env.local file

# Webhook processor authentication token
# Generate with: openssl rand -hex 32
WEBHOOK_PROCESSOR_TOKEN=your_secure_random_token_here

# Environment setting (development/production)
# This determines which webhooks get triggered
NODE_ENV=development
# or NODE_ENV=production

# Webhook processor settings
WEBHOOK_PROCESSOR_BATCH_SIZE=50
WEBHOOK_PROCESSOR_RETRY_DELAY_MS=5000