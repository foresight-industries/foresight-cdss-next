{
  "name": "@foresight-cdss-next/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run clean",
    "db:introspect": "npx drizzle-kit pull",
    "db:generate": "npx drizzle-kit generate",
    "db:push": "npx drizzle-kit push",
    "db:migrate": "npx tsx migrate.ts",
    "db:studio": "npx drizzle-kit studio --port 4000",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:reset:force": "tsx scripts/reset-db.ts --force",
    "db:seed": "tsx scripts/seed.ts",
    "db:populate-codes": "tsx scripts/populate-medical-codes.ts",
    "db:populate-carc-rarc": "tsx scripts/populate-carc-rarc-simple.ts",
    "db:populate-modifier-codes": "tsx scripts/populate-modifier-codes-simple.ts",
    "db:populate-pos-codes": "tsx scripts/populate-pos-codes.ts"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.908.0",
    "@aws-sdk/client-secrets-manager": "^3.908.0",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "*",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
    "tsx": "*",
    "typescript": "~5.8.2"
  }
}
