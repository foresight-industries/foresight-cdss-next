{
  "name": "@foresight-cdss-next/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "default": "./dist/validation.js"
    },
    "./services/twilio-sms": {
      "types": "./dist/services/twilio-sms.d.ts",
      "default": "./dist/services/twilio-sms.js"
    },
    "./services/compliance-audit": {
      "types": "./dist/services/compliance-audit.d.ts",
      "default": "./dist/services/compliance-audit.js"
    },
    "./services/bot-avoidance": {
      "types": "./dist/services/bot-avoidance.d.ts",
      "default": "./dist/services/bot-avoidance.js"
    },
    "./services/portal-automation-config": {
      "types": "./dist/services/portal-automation-config.d.ts",
      "default": "./dist/services/portal-automation-config.js"
    },
    "./services/portal-session": {
      "types": "./dist/services/portal-session.d.ts",
      "default": "./dist/services/portal-session.js"
    },
    "./services/workflow-integration": {
      "types": "./dist/services/workflow-integration.d.ts",
      "default": "./dist/services/workflow-integration.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run clean",
    "db:introspect": "npx drizzle-kit pull",
    "db:generate": "npx drizzle-kit generate",
    "db:push": "npx drizzle-kit push",
    "db:migrate": "npx tsx migrate.ts",
    "db:studio": "npx drizzle-kit studio --port 4000",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:reset:force": "tsx scripts/reset-db.ts --force",
    "db:seed": "tsx scripts/seed.ts",
    "db:populate-codes": "tsx scripts/populate-medical-codes.ts",
    "db:populate-carc-rarc": "tsx scripts/populate-carc-rarc-simple.ts",
    "db:populate-modifier-codes": "tsx scripts/populate-modifier-codes-simple.ts",
    "db:populate-pos-codes": "tsx scripts/populate-pos-codes.ts"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.908.0",
    "@aws-sdk/client-secrets-manager": "^3.908.0",
    "@browserbasehq/stagehand": "^2.5.2",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "pg": "^8.16.3",
    "playwright": "^1.56.1",
    "twilio": "^5.10.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "*",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
    "tsx": "*",
    "typescript": "~5.8.2"
  }
}
